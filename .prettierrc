{ 

    "trailingComma": "es5", 
    "tabWidth": 2, 
    "semi": false, 
    "singleQuote": true, 
    "useTabs": true, 
    "semiColons": true, 
    "printWidth": 100, 
    "bracketSpacing": true, 
    "plugins": [ 
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss" 
    ],
    "tailwindConfig": "tailwind.config.cjs",
    "pluginSearchDirs": [ 
        "." 
    ], 
    "overrides": [ 
        { 
            "files": "*.svelte", 
            "options": { 
                "parser": "svelte" 
            } 
        } 
    ] 
}